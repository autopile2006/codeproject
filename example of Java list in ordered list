import java.util.ArrayList;

public class OrderedListDemo {
    public static void main(String[] args) {
        ArrayList<String> orderedList = new ArrayList<>();
        orderedList.add("Apple");
        orderedList.add("Banana");
        orderedList.add("Cherry");

        System.out.println("Ordered List: " + orderedList);

        // Access elements by index
        System.out.println("First item: " + orderedList.get(0));
        System.out.println("Second item: " + orderedList.get(1));
        System.out.println("Third item: " + orderedList.get(2));
    }
}
